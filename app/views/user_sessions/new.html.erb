<% title "Log in" %>  
  
<% form_for @user_session do |form| %>  
<%= form.error_messages %>  
<table>
<tr><td>
    <%= form.label :username, "Username" %> </td><td>
    <%= form.text_field :username %> </td></tr>
<tr>
<td>  
    <%= form.label :password, "Password" %></td><td>  
    <%= form.password_field :password %> </td></tr>
<tr> 
	<td>&nbsp;</td>
<td align="center"> 
    <%= form.submit "Submit" %> </td></tr> 
  
</table>
<% end %>  
